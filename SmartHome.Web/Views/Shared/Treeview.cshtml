@model IEnumerable<SmartHome.Model.Models.Version>
@GetTreeView(Model)
@helper GetTreeView(IEnumerable<SmartHome.Model.Models.Version> versionModel)
{
    foreach (var nextVersion in versionModel)
    {
            <li>
                @{var vDetailModel = nextVersion.VersionDetails.Count();}
                @if (vDetailModel > 0)
                {
                    <span class="collapse collapsible">&nbsp;</span>
                }
                else
                {
                    <span style="width:15px; display:inline-block">&nbsp;</span>
                }
                <span>
                    <a href="#">@nextVersion.AppName</a>
                </span>
                @if (vDetailModel > 0)
                {
                    foreach (var nextVDetail in nextVersion.VersionDetails)
                    {
                        <ul>
                            <span style="width:15px; display:inline-block">&nbsp;</span>
                            <span>
                                <a href="#">@nextVDetail.Id</a>
                            </span>
                        </ul>
                    }

                }
            </li>
    }
}




