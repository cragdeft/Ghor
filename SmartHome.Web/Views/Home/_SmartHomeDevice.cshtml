@model List<SmartHome.Model.Models.UserHomeLink>

@foreach (var nextHome in Model)
{


    foreach (var nextRoom in nextHome.Home.Rooms)
    {
        <div >
           
            <span class="text-left">
               
                <h4 >@nextRoom.Name</h4>
            </span>
        </div>
        <hr />
        foreach (var nextDevice in nextRoom.Devices)
        {
            switch (nextDevice.DeviceType)
            {
                case DeviceType.SmartSwitch6g:
                    @*@Html.SmartSwitch6g(nextDevice)*@
                    @Html.Partial("_SmartHomeDeviceSwitch", nextDevice)
                    break;
                case DeviceType.SmartRainbow12:
                    @Html.Partial("_SmartHomeDeviceRGBW")
                    break;
                case DeviceType.Curtain:

                    break;
                case DeviceType.Camera:
                    break;

            }
        }

    }

}

